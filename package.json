{
  "name": "os-window-vue",
  "version": "0.1.5",
  "description": "Integrates the os-window custom component into vue.",
  "main": "main.js",
  "scripts": {
    "lint": "vue-cli-service lint --no-fix browser.js main.js lib test",
    "dev": "python -m SimpleHTTPServer 8000",
    "test": "node ./test/runner.js",
    "prerelease": "rm -rf node_modules && yarn && npx npm-install-peers && npm login",
    "release": "np --no-cleanup",
    "prepare": "husky install"
  },
  "keywords": [
    "window",
    "chrome",
    "decoration",
    "border",
    "os",
    "operating system",
    "mac",
    "macos",
    "apple",
    "os-window",
    "vue",
    "vuejs",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/benjaminsattler/os-window-vue.git"
  },
  "bugs": {
    "url": "https://github.com/benjaminsattler/os-window-vue/issues"
  },
  "author": {
    "name": "Benjamin Sattler",
    "email": "bsattler.inbox@gmail.com"
  },
  "homepage": "https://os-window.benjaminsattler.net",
  "license": "MIT",
  "peerDependencies": {
    "os-window": "^0.2.0 || ^0.3.0",
    "vue": "^2.0.0"
  },
  "peerInstallOptions": {
    "save": false
  },
  "files": [
    "lib",
    "main.js",
    "browser.js"
  ],
  "devDependencies": {
    "@vue/cli": "^4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/test-utils": "^1.2.0",
    "chai": "^4.3.4",
    "connect": "^3.7.0",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.11.0",
    "husky": "^6.0.0",
    "mocha": "^9.0.0",
    "np": "^7.5.0",
    "npm-install-peers": "^1.2.2",
    "playwright": "^1.12.1",
    "prettier": "^2.3.1",
    "serve-static": "^1.14.1",
    "vue-template-compiler": "^2.6.14"
  },
  "directories": {
    "lib": "./lib",
    "doc": "./doc",
    "test": "./test"
  }
}
